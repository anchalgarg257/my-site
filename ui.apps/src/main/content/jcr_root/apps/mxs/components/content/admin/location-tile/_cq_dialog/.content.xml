<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <dealerCode
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Store/Dealer Code"
                                        name="./dealerCode"
                                        required="{Boolean}true"/>
                                    <realViewCode
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Real View Code"
                                        name="./realViewCode"
                                        required="{Boolean}true"/>
                                    <storeName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Store Name"
                                        name="./storeName"
                                        required="{Boolean}true"/>
                                    <titleonwebsite
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title on Website "
                                        name="./title"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <address
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Address"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <region
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Region"
                                        name="./region"
                                        required="{Boolean}true"/>
                                    <state
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="State"
                                        name="./state"
                                        required="{Boolean}true"/>
                                    <city
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="City/Town"
                                        name="./town"
                                        required="{Boolean}true"/>
                                    <address
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Address"
                                        name="./address"
                                        required="{Boolean}true"/>
                                    <postalCode
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldLabel="Postal Code"
                                        name="./postal"
                                        required="{Boolean}true"/>
                                    <latitude
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Latitude"
                                        name="./latitude"
                                        required="{Boolean}true"/>
                                    <longitude
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Longitude"
                                        name="./longitude"
                                        required="{Boolean}true"/>
                                    <googleMapLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Google Map Link"
                                        name="./googleMapLink"
                                        required="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </address>
                    <advance
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Opening Hours"
                                        name="./openingHours"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline]">
                                                    <popovers jcr:primaryType="nt:unstructured"/>
                                                </inline>
                                            </cui>
                                        </uiSettings>
                                        <htmlRules jcr:primaryType="nt:unstructured">
                                            <blockHandling
                                                jcr:primaryType="nt:unstructured"
                                                removeSingleParagraphContainer="{Boolean}true"/>
                                        </htmlRules>
                                    </description>
                                    <self-collection
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        name="./selfCollection"
                                        text="Self Collection"
                                        value="selfCollection"/>
                                    <type-of-product
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Type of Product Sold Tagging"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <tags
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/common/tagspicker"
                                                fieldLabel="Choose Stickers"
                                                name="./serviceTags"
                                                rootPath="/content/cq:tags"/>
                                        </items>
                                    </type-of-product>
                                    <new-store
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/switch"
                                        fieldLabel="New Store"
                                        name="./isNewStore"
                                        offText="No"
                                        onText="Yes"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <mini-store
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/switch"
                                        fieldLabel="Mini Store"
                                        name="./isMiniStore"
                                        offText="No"
                                        onText="Yes"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column>
                        </items>
                    </advance>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
